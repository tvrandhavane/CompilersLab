	sw $fp, 0($sp)		# Save the frame pointer
	sub $fp, $sp, 4		# Update the frame pointer

	sub $sp, $sp, 20		# Make space for the locals
# Prologue ends


label2:    	
	li.d $f2, 2.30
	s.d $f2, -8($fp)
	li.d $f2, 3.20
	s.d $f2, 0($fp)
	l.d $f2, 0($fp)
	li.d $f4, 2.10
	div.d $f6, $f2, $f4
	l.d $f2, -8($fp)
	sub.d $f4, $f6, $f2
	s.d $f4, -8($fp)
	l.d $f2, -8($fp)
	mfc1 $v0, $f2
	move $v1, $v0

return:    	

